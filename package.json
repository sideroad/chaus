{
  "name": "chaus",
  "title": "chaus",
  "description": "Build RESTful API within 5 min",
  "author": "sideroad",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sideroad/chaus.git"
  },
  "homepage": "https://github.com/sideroad/chaus",
  "keywords": [
    "express",
    "RESTful",
    "API",
    "starter"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "better-npm-run prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "dev": "concurrent --kill-others \"better-npm-run watch\" \"better-npm-run dev\"",
    "test": "karma start"
  },
  "betterScripts": {
    "prod": {
      "command": "node bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "GLOBAL_HOST": "chaus.herokuapp.com",
        "GLOBAL_PORT": 443
      }
    },
    "watch": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    },
    "dev": {
      "command": "node bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": "3000"
      }
    }
  },
