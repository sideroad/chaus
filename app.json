{
  "name": "chaus",
  "description": "chaus",
  "repository": "https://github.com/sideroad/chaus",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "env": {
    "NODE_ENV": {
      "value": "production"
    },
    "NODE_PATH": {
      "value": "./src"
    },
    "NPM_CONFIG_PRODUCTION": {
      "value": "false"
    },
    "GLOBAL_HOST": {
      "description": "Please specify domain"
    },
    "GLOBAL_PORT": {
      "value": "443"
    },
    "CHAUS_GITHUB_CLIENT_ID": {
      "description": "Please specify github client ID"
    },
    "CHAUS_GITHUB_CLIENT_SECRET": {
      "description": "Please specify github client secret ID"
    }
  },
  "addons": [
    "mongolab:sandbox"
  ]
}
